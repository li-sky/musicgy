{
  "name": "app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "next dev --webpack",
    "build": "next build",
    "build:optimized": "npm run clean && node scripts/pre-build.js && next build && npm run analyze && npm run clean:unused",
    "build:analyze": "npm run analyze-deps && npm run build:optimized",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "upload": "opennextjs-cloudflare upload",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "start": "next start",
    "lint": "next lint",
    "clean": "rimraf .next .open-next node_modules/.cache scripts/mock",
    "clean:unused": "rimraf .open-next/server-functions/default/handler.mjs.map && rimraf .open-next/server-functions/default/*.map",
    "analyze": "node scripts/optimize-build.js",
    "analyze-deps": "node scripts/analyze-deps.js"
  },
  "dependencies": {
    "@neteasecloudmusicapienhanced/api": "^4.29.19",
    "@opennextjs/cloudflare": "^1.14.7",
    "autoprefixer": "^10.4.23",
    "body-parser": "^2.2.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^5.2.1",
    "express-rate-limit": "^7.1.3",
    "helmet": "^7.1.0",
    "ioredis": "^5.8.2",
    "next": "^16.1.1",
    "node-fetch": "^3.3.2",
    "postcss": "^8.5.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "stream": "^0.0.3",
    "tailwindcss": "^3.4.19"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "typescript": "^5.9.3",
    "vercel": "^50.1.3",
    "vite": "^7.3.0",
    "wrangler": "^4.54.0"
  }
}
